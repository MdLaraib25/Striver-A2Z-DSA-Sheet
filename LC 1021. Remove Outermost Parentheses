class Solution {
    public String removeOuterParentheses(String s) {
         int length = s.length();
         StringBuilder ans = new StringBuilder();
         int cnt = 0;
         for( int i = 0; i < length; i++){
            if (s.charAt(i) == '('){
                if (cnt > 0) ans.append(s.charAt(i));
                cnt++;
            } else {
                cnt--;
                if(cnt > 0) ans.append(s.charAt(i));
            }
         } return ans.toString();
    }
}
