class Solution {
    public boolean isAnagram(String s, String t) {
        int[] total = new int[26];
        for(char ch: s.toCharArray()){
            total[ch -'a']++;
        }
        for(char ch: t.toCharArray()){
            total[ch -'a']--;
        }
        for(int num : total){
            if(num != 0) return false;
        }return true;
    }
}
